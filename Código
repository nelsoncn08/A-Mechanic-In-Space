#include <stdio.h>
#include <allegro5/allegro.h>
#include <allegro5/allegro_image.h>
#include <allegro5/allegro_audio.h>
#include <allegro5/allegro_acodec.h>
#include <unistd.h>
#include <time.h>
#include <math.h>

const float FPS = 90;
const int SCREEN_W = 1024;
const int SCREEN_H = 768;

const int SATELITE1_LARGURA = 252;
const int SATELITE1_ALTURA = 214;
const int SATELITE2_LARGURA = 252;
const int SATELITE2_ALTURA = 216;
const int SATELITE3_LARGURA = 252;
const int SATELITE3_ALTURA = 296;
const int SATELITE4_LARGURA = 252;
const int SATELITE4_ALTURA = 406;
const int HATCH_LARGURA = 100;
const int HATCH_ALTURA = 20;
const int ASTRONAUTA_LARGURA = 76;
const int ASTRONAUTA_ALTURA = 112;
const int LAPTOP_LARGURA = 972;
const int LAPTOP_ALTURA = 730;


int main(int argc, char **argv)
{

ALLEGRO_DISPLAY *display = NULL;
ALLEGRO_EVENT_QUEUE *event_queue = NULL;
ALLEGRO_TIMER *timer = NULL;

if(!al_init())
{
    fprintf(stderr, "failed to initialize allegro!\n");
    return -1;
}

timer = al_create_timer(1.0 / FPS);

if(!timer)
{
    fprintf(stderr, "failed to create timer!\n");
    return -1;
}

bool redraw = true;

al_set_new_display_flags(ALLEGRO_OPENGL | ALLEGRO_WINDOWED);

display = al_create_display(SCREEN_W, SCREEN_H);

al_set_window_title(display, "Um Mecanico No Espaco");

al_init_font_addon();
al_init_ttf_addon();

al_install_audio();
al_init_acodec_addon();

al_reserve_samples(8);

if(!display)
{
    fprintf(stderr, "failed to create display!\n");
    al_destroy_timer(timer);
    return -1;
}

if(!al_install_keyboard())
{
    fprintf(stderr, "failed to initialize the keyboard!\n");
    return -1;
}

if(!al_install_mouse())
{
    fprintf(stderr, "failed to initialize the mouse!\n");
    return -1;
}

if(!al_init_image_addon())
{
    fprintf(stderr, "failed to initialize the image addon!\n");
    return -1;
}

ALLEGRO_BITMAP *satelite1 = NULL;
ALLEGRO_BITMAP *satelite2 = NULL;
ALLEGRO_BITMAP *satelite3 = NULL;
ALLEGRO_BITMAP *satelite4 = NULL;
ALLEGRO_BITMAP *missao1 = NULL;
ALLEGRO_BITMAP *missao2 = NULL;
ALLEGRO_BITMAP *missao3 = NULL;
ALLEGRO_BITMAP *missao4 = NULL;
ALLEGRO_BITMAP *astronautaesq = NULL;
ALLEGRO_BITMAP *astronautadir = NULL;
ALLEGRO_BITMAP *background = NULL;
ALLEGRO_BITMAP *hatch = NULL;
ALLEGRO_BITMAP *imgmenu = NULL;
ALLEGRO_BITMAP *imginstrucoes = NULL;
ALLEGRO_BITMAP *laptop = NULL;
ALLEGRO_BITMAP *imgsair = NULL;
ALLEGRO_BITMAP *imgderrota = NULL;
ALLEGRO_BITMAP *imgvitoria = NULL;
ALLEGRO_BITMAP *imgvitoriad = NULL;
ALLEGRO_BITMAP *imggg = NULL;
ALLEGRO_BITMAP *tecla1 = NULL;
ALLEGRO_BITMAP *tecla2 = NULL;
ALLEGRO_BITMAP *tecla3 = NULL;
ALLEGRO_BITMAP *tecla4 = NULL;
ALLEGRO_BITMAP *tecla5 = NULL;
ALLEGRO_BITMAP *tecla6 = NULL;
ALLEGRO_BITMAP *tecla7 = NULL;
ALLEGRO_BITMAP *tecla8 = NULL;
ALLEGRO_BITMAP *tecla9 = NULL;
ALLEGRO_BITMAP *teclaQ = NULL;
ALLEGRO_BITMAP *teclaW = NULL;
ALLEGRO_BITMAP *teclaE = NULL;
ALLEGRO_BITMAP *teclaR = NULL;
ALLEGRO_BITMAP *teclaT = NULL;
ALLEGRO_BITMAP *teclaY = NULL;
ALLEGRO_BITMAP *teclaU = NULL;
ALLEGRO_BITMAP *teclaI = NULL;
ALLEGRO_BITMAP *teclaO = NULL;
ALLEGRO_BITMAP *teclaP = NULL;
ALLEGRO_BITMAP *teclaA = NULL;
ALLEGRO_BITMAP *teclaS = NULL;
ALLEGRO_BITMAP *teclaD = NULL;
ALLEGRO_BITMAP *teclaF = NULL;
ALLEGRO_BITMAP *teclaG = NULL;
ALLEGRO_BITMAP *teclaH = NULL;
ALLEGRO_BITMAP *teclaJ = NULL;
ALLEGRO_BITMAP *teclaK = NULL;
ALLEGRO_BITMAP *teclaL = NULL;
ALLEGRO_BITMAP *teclaZ = NULL;
ALLEGRO_BITMAP *teclaX = NULL;
ALLEGRO_BITMAP *teclaC = NULL;
ALLEGRO_BITMAP *teclaV = NULL;
ALLEGRO_BITMAP *teclaB = NULL;
ALLEGRO_BITMAP *teclaN = NULL;
ALLEGRO_BITMAP *teclaM = NULL;
ALLEGRO_BITMAP *imgcheck = NULL;
ALLEGRO_BITMAP *imgblack = NULL;

ALLEGRO_SAMPLE *somtrilha = NULL;
ALLEGRO_SAMPLE *somcerto = NULL;
ALLEGRO_SAMPLE *somerrado = NULL;
ALLEGRO_SAMPLE *somderrota = NULL;
ALLEGRO_SAMPLE *somvitoriad = NULL;
ALLEGRO_SAMPLE *somvitoria = NULL;
ALLEGRO_SAMPLE *somgg = NULL;

ALLEGRO_SAMPLE *inst_somtrilha = NULL;
ALLEGRO_SAMPLE *inst_somcerto = NULL;
ALLEGRO_SAMPLE *inst_somerrado = NULL;
ALLEGRO_SAMPLE *inst_somderrota = NULL;
ALLEGRO_SAMPLE *inst_somvitoriad = NULL;
ALLEGRO_SAMPLE *inst_somvitoria = NULL;
ALLEGRO_SAMPLE *inst_somgg = NULL;

float hatch_pos_x = SCREEN_W - HATCH_LARGURA;
float hatch_pos_y = SCREEN_H - HATCH_ALTURA;
float satelite1_pos_x = 150;
float satelite1_pos_y = 80;
float satelite2_pos_x = 100;
float satelite2_pos_y = 150;
float satelite3_pos_x = 40;
float satelite3_pos_y = 400;
float satelite4_pos_x = 10;
float satelite4_pos_y = 10;
float astronauta_pos_x = SCREEN_W - HATCH_LARGURA / 2.0 - ASTRONAUTA_LARGURA / 2.0;
float astronauta_pos_y = SCREEN_H;
float laptop_pos_x = SCREEN_W / 2.0 - LAPTOP_LARGURA / 2.0;
float laptop_pos_y = SCREEN_H / 2.0 - LAPTOP_ALTURA / 2.0;
float pos_gg_y = 763;

float astronauta_vy = 0;
float astronauta_vx = 0;

satelite1 = al_load_bitmap("imagens/satelite1.png");
satelite2 = al_load_bitmap("imagens/satelite2.png");
satelite3 = al_load_bitmap("imagens/satelite3.png");
satelite4 = al_load_bitmap("imagens/satelite4.png");
missao1 = al_load_bitmap("imagens/missao1.png");
missao2 = al_load_bitmap("imagens/missao2.png");
missao3 = al_load_bitmap("imagens/missao3.png");
missao4 = al_load_bitmap("imagens/missao4.png");
astronautaesq = al_load_bitmap("imagens/astronauta_esq.png");
astronautadir = al_load_bitmap("imagens/astronauta_dir.png");
background = al_load_bitmap("imagens/espaco.png");
hatch = al_load_bitmap("imagens/hatch.png");
imgmenu = al_load_bitmap("imagens/menu.png");
imginstrucoes = al_load_bitmap("imagens/instrucoes.png");
laptop = al_load_bitmap("imagens/laptop.png");
imgsair = al_load_bitmap("imagens/final.png");
imgderrota = al_load_bitmap("imagens/derrota.png");
imgvitoria = al_load_bitmap("imagens/vitoria.png");
imgvitoriad = al_load_bitmap("imagens/vitoriad.png");
imggg = al_load_bitmap("imagens/gg.png");
tecla1 = al_load_bitmap("imagens/tecla1.png");
tecla2 = al_load_bitmap("imagens/tecla2.png");
tecla3 = al_load_bitmap("imagens/tecla3.png");
tecla4 = al_load_bitmap("imagens/tecla4.png");
tecla5 = al_load_bitmap("imagens/tecla5.png");
tecla6 = al_load_bitmap("imagens/tecla6.png");
tecla7 = al_load_bitmap("imagens/tecla7.png");
tecla8 = al_load_bitmap("imagens/tecla8.png");
tecla9 = al_load_bitmap("imagens/tecla9.png");
teclaQ = al_load_bitmap("imagens/teclaQ.png");
teclaW = al_load_bitmap("imagens/teclaW.png");
teclaE = al_load_bitmap("imagens/teclaE.png");
teclaR = al_load_bitmap("imagens/teclaR.png");
teclaT = al_load_bitmap("imagens/teclaT.png");
teclaY = al_load_bitmap("imagens/teclaY.png");
teclaU = al_load_bitmap("imagens/teclaU.png");
teclaI = al_load_bitmap("imagens/teclaI.png");
teclaO = al_load_bitmap("imagens/teclaO.png");
teclaP = al_load_bitmap("imagens/teclaP.png");
teclaA = al_load_bitmap("imagens/teclaA.png");
teclaS = al_load_bitmap("imagens/teclaS.png");
teclaD = al_load_bitmap("imagens/teclaD.png");
teclaF = al_load_bitmap("imagens/teclaF.png");
teclaG = al_load_bitmap("imagens/teclaG.png");
teclaH = al_load_bitmap("imagens/teclaH.png");
teclaJ = al_load_bitmap("imagens/teclaJ.png");
teclaK = al_load_bitmap("imagens/teclaK.png");
teclaL = al_load_bitmap("imagens/teclaL.png");
teclaZ = al_load_bitmap("imagens/teclaZ.png");
teclaX = al_load_bitmap("imagens/teclaX.png");
teclaC = al_load_bitmap("imagens/teclaC.png");
teclaV = al_load_bitmap("imagens/teclaV.png");
teclaB = al_load_bitmap("imagens/teclaB.png");
teclaN = al_load_bitmap("imagens/teclaN.png");
teclaM = al_load_bitmap("imagens/teclaM.png");
imgcheck = al_load_bitmap("imagens/check.png");
imgblack = al_load_bitmap("imagens/black.png");

somtrilha = al_load_sample("sons/trilha.wav");
somcerto = al_load_sample("sons/certo.wav");
somerrado = al_load_sample("sons/errado.wav");
somderrota = al_load_sample("sons/derrota.wav");
somvitoriad = al_load_sample("sons/vitoriad.wav");
somvitoria = al_load_sample("sons/vitoria.wav");
somgg = al_load_sample("sons/gg.wav");

inst_somtrilha = al_create_sample_instance(somtrilha);
inst_somcerto = al_create_sample_instance(somcerto);
inst_somerrado = al_create_sample_instance(somerrado);
inst_somderrota = al_create_sample_instance(somderrota);
inst_somvitoriad = al_create_sample_instance(somvitoriad);
inst_somvitoria = al_create_sample_instance(somvitoria);
inst_somgg = al_create_sample_instance(somgg);

al_attach_sample_instance_to_mixer(inst_somtrilha, al_get_default_mixer());
al_attach_sample_instance_to_mixer(inst_somcerto, al_get_default_mixer());
al_attach_sample_instance_to_mixer(inst_somerrado, al_get_default_mixer());
al_attach_sample_instance_to_mixer(inst_somderrota, al_get_default_mixer());
al_attach_sample_instance_to_mixer(inst_somvitoriad, al_get_default_mixer());
al_attach_sample_instance_to_mixer(inst_somvitoria, al_get_default_mixer());
al_attach_sample_instance_to_mixer(inst_somgg, al_get_default_mixer());

al_set_sample_instance_playmode(inst_somtrilha, ALLEGRO_PLAYMODE_LOOP);
al_set_sample_instance_gain(inst_somtrilha, 1.0);
al_set_sample_instance_playmode(inst_somgg, ALLEGRO_PLAYMODE_LOOP);
al_set_sample_instance_gain(inst_somgg, 1.0);

if(!satelite1)
{
    fprintf(stderr, "falhou ao criar o satelite1 bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    return -1;
}

if(!satelite2)
{
    fprintf(stderr, "falhou ao criar o satelite2 bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    return -1;
}

if(!satelite3)
{
    fprintf(stderr, "falhou ao criar o satelite3 bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    return -1;
}

if(!satelite4)
{
    fprintf(stderr, "falhou ao criar o satelite4 bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    return -1;
}

if(!missao1)
{
    fprintf(stderr, "falhou ao criar o missao1 bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    return -1;
}

if(!missao2)
{
    fprintf(stderr, "falhou ao criar o missao2 bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    return -1;
}

if(!missao3)
{
    fprintf(stderr, "falhou ao criar o missao3 bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    return -1;
}

if(!missao4)
{
    fprintf(stderr, "falhou ao criar o missao4 bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    return -1;
}

if(!astronautaesq)
{
    fprintf(stderr, "falhou ao criar o astronauta esquerda bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    return -1;
}

if(!astronautadir)
{
    fprintf(stderr, "falhou ao criar o astronauta direita bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    return -1;
}

if(!background)
{
    fprintf(stderr, "falhou ao criar o background bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    al_destroy_bitmap(astronautadir);
    return -1;
}

if(!hatch)
{
    fprintf(stderr, "falhou ao criar o background bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    al_destroy_bitmap(astronautadir);
    al_destroy_bitmap(background);
    return -1;
}

if(!imgmenu)
{
    fprintf(stderr, "falhou ao criar o imgmenu bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    al_destroy_bitmap(astronautadir);
    al_destroy_bitmap(background);
    al_destroy_bitmap(hatch);
    return -1;
}

if(!imginstrucoes)
{
    fprintf(stderr, "falhou ao criar o imginstrucoes bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    al_destroy_bitmap(astronautadir);
    al_destroy_bitmap(background);
    al_destroy_bitmap(hatch);
    al_destroy_bitmap(imgmenu);
    return -1;
}

if(!laptop)
{
    fprintf(stderr, "falhou ao criar o laptop bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    al_destroy_bitmap(astronautadir);
    al_destroy_bitmap(background);
    al_destroy_bitmap(hatch);
    al_destroy_bitmap(imgmenu);
    al_destroy_bitmap(imginstrucoes);
    return -1;
}

if(!imgsair)
{
    fprintf(stderr, "falhou ao criar o imgsair bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    al_destroy_bitmap(astronautadir);
    al_destroy_bitmap(background);
    al_destroy_bitmap(hatch);
    al_destroy_bitmap(imgmenu);
    al_destroy_bitmap(imginstrucoes);
    al_destroy_bitmap(laptop);
    return -1;
}

if(!imgderrota)
{
    fprintf(stderr, "falhou ao criar o imgderrota bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    al_destroy_bitmap(astronautadir);
    al_destroy_bitmap(background);
    al_destroy_bitmap(hatch);
    al_destroy_bitmap(imgmenu);
    al_destroy_bitmap(imginstrucoes);
    al_destroy_bitmap(laptop);
    al_destroy_bitmap(imgsair);
    return -1;
}

if(!imgvitoria)
{
    fprintf(stderr, "falhou ao criar o imgvitoria bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    al_destroy_bitmap(astronautadir);
    al_destroy_bitmap(background);
    al_destroy_bitmap(hatch);
    al_destroy_bitmap(imgmenu);
    al_destroy_bitmap(imginstrucoes);
    al_destroy_bitmap(laptop);
    al_destroy_bitmap(imgsair);
    al_destroy_bitmap(imgderrota);
    return -1;
}

if(!imgvitoriad)
{
    fprintf(stderr, "falhou ao criar o imgvitoriad bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    al_destroy_bitmap(astronautadir);
    al_destroy_bitmap(background);
    al_destroy_bitmap(hatch);
    al_destroy_bitmap(imgmenu);
    al_destroy_bitmap(imginstrucoes);
    al_destroy_bitmap(laptop);
    al_destroy_bitmap(imgsair);
    al_destroy_bitmap(imgderrota);
    al_destroy_bitmap(imgvitoria);
    return -1;
}

if(!imggg)
{
    fprintf(stderr, "falhou ao criar o imggg bitmap!\n");
    al_destroy_display(display);
    al_destroy_timer(timer);
    al_destroy_bitmap(satelite1);
    al_destroy_bitmap(satelite2);
    al_destroy_bitmap(satelite3);
    al_destroy_bitmap(satelite4);
    al_destroy_bitmap(missao1);
    al_destroy_bitmap(missao2);
    al_destroy_bitmap(missao3);
    al_destroy_bitmap(missao4);
    al_destroy_bitmap(astronautaesq);
    al_destroy_bitmap(astronautadir);
    al_destroy_bitmap(background);
    al_destroy_bitmap(hatch);
