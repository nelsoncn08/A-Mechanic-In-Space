                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c26)
                {
                    if(TECLAS[11]==34)
                    {
                        c26 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc26 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c31)
                {
                    if(TECLAS[12]==34)
                    {
                        c31 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc31 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c32)
                {
                    if(TECLAS[13]==34)
                    {
                        c32 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc32 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c33)
                {
                    if(TECLAS[14]==34)
                    {
                        c33 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc33 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c34)
                {
                    if(TECLAS[15]==34)
                    {
                        c34 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc34 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c35)
                {
                    if(TECLAS[16]==34)
                    {
                        c35 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc35 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c36)
                {
                    if(TECLAS[17]==34)
                    {
                        c36 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc36 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c41)
                {
                    if(TECLAS[18]==34)
                    {
                        c41 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
