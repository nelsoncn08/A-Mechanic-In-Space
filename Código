        else if(ev.type == ALLEGRO_EVENT_KEY_DOWN)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_Z:
                if(c11)
                {
                    if(TECLAS[0]==0)
                    {
                        c11 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc11 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
