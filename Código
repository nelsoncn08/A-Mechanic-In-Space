                    break;

                case 4:
                    d11 = true;
                    d12 = true;
                    d13 = true;
                    d14 = true;
                    d15 = true;
                    d16 = true;
                    d21 = true;
                    d22 = true;
                    d23 = true;
                    d24 = true;
                    d25 = true;
                    d26 = true;
                    d31 = true;
                    d32 = true;
                    d33 = true;
                    d34 = true;
                    d35 = true;
                    d36 = true;
                    d41 = true;
                    d42 = true;
                    d43 = true;
                    d44 = true;
                    d45 = true;
                    d46 = true;
                    fim4 = true;
                    break;
                }
                posicoes = true;
                pos = true;
            }

            if(pos)
            {
                switch(level)
                {
                case 1:
                    repeticoes = 6;
                    break;
                case 2:
                    repeticoes = 12;
                    break;
                case 3:
                    repeticoes = 18;
                    break;
                case 4:
                    repeticoes = 24;
                    break;
                }
                for ( ; contador < repeticoes; contador += 1)
                {
                    switch(level)
                    {
                    case 1:
                        nr = rand() % 7;
                        break;
                    case 2:
                        nr = rand() % 16;
                        break;
                    case 3:
                        nr = rand() % 26;
                        break;
                    case 4:
                        nr = rand() % 35;
                        break;
                    }
                    TECLAS[contador] = nr;
                }
                switch(level)
                {
                case 1:
                    c11=true;
                    c12=true;
                    c13=true;
                    c14=true;
                    c15=true;
                    c16=true;
                    break;
                case 2:
                    c11=true;
                    c12=true;
                    c13=true;
                    c14=true;
                    c15=true;
                    c16=true;
                    c21=true;
                    c22=true;
                    c23=true;
                    c24=true;
                    c25=true;
                    c26=true;
                    break;
                case 3:
                    c11=true;
                    c12=true;
                    c13=true;
                    c14=true;
                    c15=true;
                    c16=true;
                    c21=true;
                    c22=true;
                    c23=true;
                    c24=true;
                    c25=true;
                    c26=true;
                    c31=true;
                    c32=true;
                    c33=true;
                    c34=true;
                    c35=true;
                    c36=true;
                    break;
                case 4:
                    c11=true;
                    c12=true;
                    c13=true;
                    c14=true;
                    c15=true;
                    c16=true;
                    c21=true;
                    c22=true;
                    c23=true;
                    c24=true;
                    c25=true;
                    c26=true;
                    c31=true;
                    c32=true;
                    c33=true;
                    c34=true;
                    c35=true;
                    c36=true;
                    c41=true;
                    c42=true;
                    c43=true;
                    c44=true;
                    c45=true;
                    c46=true;
                    break;
                }
                pos = false;
            }

            if(d11)
