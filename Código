                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c42)
                {
                    if(TECLAS[19]==34)
                    {
                        c42 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc42 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c43)
                {
                    if(TECLAS[20]==34)
                    {
                        c43 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc43 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c44)
                {
                    if(TECLAS[21]==34)
                    {
                        c44 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc44 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c45)
                {
                    if(TECLAS[22]==34)
                    {
                        c45 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc45 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }
                else if(c46)
                {
                    if(TECLAS[23]==34)
                    {
                        c46 = false;
                        al_stop_sample_instance(inst_somcerto);
                        al_play_sample_instance(inst_somcerto);
                        dc46 = true;
                        drawreparo = true;
                    }
                    else
                    {
                        penalidade += 1;
                        al_stop_sample_instance(inst_somerrado);
                        al_play_sample_instance(inst_somerrado);
                    }
                    break;
                }

                break;

            case ALLEGRO_KEY_ESCAPE:
                fim = true;
                reparo = false;
                break;
            }
        }
    }

    while(fim)
    {
        ALLEGRO_EVENT ev;

        al_wait_for_event(event_queue, &ev);
        al_show_mouse_cursor(display);

        if(ev.type == ALLEGRO_EVENT_TIMER)
        {
            if(resultado == 1) //vitoria
            {
                al_clear_to_color(al_map_rgb(0,100,0));

                if(penalidade == 0) //sem prejuizo
                {
                    al_draw_bitmap(imgvitoria, 0, 0, 0);

                    if(para_bens)
                    {
                        sleep(0.5);
                        al_play_sample_instance(inst_somvitoria);
                        para_bens = false;
                    }
                }
                else //com prejuizo
                {
                    al_draw_bitmap(imgvitoriad, 0, 0, 0);

                    if(e_tabom)
                    {
                        sleep(0.5);
                        al_play_sample_instance(inst_somvitoriad);
                        e_tabom = false;
                    }
                }
                al_flip_display();
            }
            else if(resultado == 2) //derrota
            {
                al_clear_to_color(al_map_rgb(0,100,0));
                al_draw_bitmap(imgderrota, 0, 0, 0);
                al_flip_display();

                if(vergonha)
                {
                    sleep(0.5);
                    al_play_sample_instance(inst_somderrota);
                    vergonha = false;
                }
            }
            else if(resultado == 3) //GG
            {
                endgame = true;
                fim = false;
                break;
            }
            else if(resultado == 0) //sair
            {
                al_stop_sample_instance(inst_somderrota);
                al_stop_sample_instance(inst_somvitoriad);
                al_stop_sample_instance(inst_somvitoria);
                al_stop_sample_instance(inst_somtrilha);
                al_stop_sample_instance(inst_somgg);

                al_clear_to_color(al_map_rgb(0,100,0));
                al_draw_bitmap(imgsair, 0, 0, 0);
                al_flip_display();
            }
        }
        else if(ev.type==ALLEGRO_EVENT_MOUSE_BUTTON_DOWN)
        {
            switch(ev.mouse.button)
            {
            case 1:
                if(ev.mouse.x >= 129 && ev.mouse.y >= 603 && ev.mouse.x <= 447 && ev.mouse.y <= 649)
                {
                    if(resultado > 0) // CONTINUAR JOGANDO
                    {
                        // VARIAVEIS
                        nr = 0;
                        penalidade = 0;

                        d11Z = false;
                        d12Z = false;
                        d13Z = false;
                        d14Z = false;
                        d15Z = false;
                        d16Z = false;
                        d21Z = false;
                        d22Z = false;
