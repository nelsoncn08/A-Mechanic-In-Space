                        d369 = false;
                        d419 = false;
                        d429 = false;
                        d439 = false;
                        d449 = false;
                        d459 = false;
                        d469 = false;
                        dc11 = false;
                        dc12 = false;
                        dc13 = false;
                        dc14 = false;
                        dc15 = false;
                        dc16 = false;
                        dc21 = false;
                        dc22 = false;
                        dc23 = false;
                        dc24 = false;
                        dc25 = false;
                        dc26 = false;
                        dc31 = false;
                        dc32 = false;
                        dc33 = false;
                        dc34 = false;
                        dc35 = false;
                        dc36 = false;
                        dc41 = false;
                        dc42 = false;
                        dc43 = false;
                        dc44 = false;
                        dc45 = false;
                        dc46 = false;

                        drawreparo = true;
                        posicoes = false;
                        fim1 = false;
                        fim2 = false;
                        fim3 = false;
                        fim4 = false;
                        contador = 0;
                        repeticoes = 0;

                        if(resultado == 1) // PRXIMA FASE
                        {
                            level += 1;
                            introducao = true;
                        }
                        else // TENTAR NOVAMENTE
                        {
                            astronauta_pos_x = SCREEN_W - HATCH_LARGURA / 2.0 - ASTRONAUTA_LARGURA / 2.0;
                            astronauta_pos_y = SCREEN_H;
                            saida = true;
                        }

                        fim = false;

                        al_stop_sample_instance(inst_somderrota);
                        al_stop_sample_instance(inst_somvitoriad);
                        al_stop_sample_instance(inst_somvitoria);

                        break;
                    }
                }
                else if(ev.mouse.x >= 591 && ev.mouse.y >= 603 && ev.mouse.x <= 909 && ev.mouse.y <= 649) // SAIR
                {
                    resultado = 0;
                    break;
                }
            }
        }
        else if(ev.type == ALLEGRO_EVENT_KEY_DOWN)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_ESCAPE:
                return 0;
                break;
            }
        }
        else if(ev.type == ALLEGRO_EVENT_DISPLAY_CLOSE)
        {
            break;
        }
    }

    while(endgame)
    {
        ALLEGRO_EVENT ev;

        al_wait_for_event(event_queue, &ev);
        al_hide_mouse_cursor(display);

        if(ev.type == ALLEGRO_EVENT_TIMER)
        {
            al_clear_to_color(al_map_rgb(0,0,0));
            al_draw_bitmap(imgblack, 0, 0, 0);
            al_draw_bitmap(imggg, 0, pos_gg_y, 0);
            al_flip_display();

            al_play_sample_instance(inst_somgg);

            for( ; pos_gg_y >= 0; pos_gg_y -= 1)
            {
                al_draw_bitmap(imgblack, 0, 0, 0);
                al_draw_bitmap(imggg, 0, pos_gg_y, 0);
                sleep(0.3);
                al_flip_display();
            }
        }
        else if(ev.type == ALLEGRO_EVENT_KEY_DOWN)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_ESCAPE:
                return 0;
                break;
            }
        }
        else if(ev.type == ALLEGRO_EVENT_DISPLAY_CLOSE)
        {
            break;
        }
    }
}

al_destroy_display(display);
al_destroy_timer(timer);
