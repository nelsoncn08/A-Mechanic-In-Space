bool d111 = false, d121 = false, d131 = false, d141 = false, d151 = false, d161 = false, d211 = false, d221 = false, d231 = false, d241 = false, d251 = false, d261 = false, d311 = false, d321 = false, d331 = false, d341 = false, d351 = false, d361 = false, d411 = false, d421 = false, d431 = false, d441 = false, d451 = false, d461 = false;
bool d112 = false, d122 = false, d132 = false, d142 = false, d152 = false, d162 = false, d212 = false, d222 = false, d232 = false, d242 = false, d252 = false, d262 = false, d312 = false, d322 = false, d332 = false, d342 = false, d352 = false, d362 = false, d412 = false, d422 = false, d432 = false, d442 = false, d452 = false, d462 = false;
bool d113 = false, d123 = false, d133 = false, d143 = false, d153 = false, d163 = false, d213 = false, d223 = false, d233 = false, d243 = false, d253 = false, d263 = false, d313 = false, d323 = false, d333 = false, d343 = false, d353 = false, d363 = false, d413 = false, d423 = false, d433 = false, d443 = false, d453 = false, d463 = false;
bool d114 = false, d124 = false, d134 = false, d144 = false, d154 = false, d164 = false, d214 = false, d224 = false, d234 = false, d244 = false, d254 = false, d264 = false, d314 = false, d324 = false, d334 = false, d344 = false, d354 = false, d364 = false, d414 = false, d424 = false, d434 = false, d444 = false, d454 = false, d464 = false;
bool d115 = false, d125 = false, d135 = false, d145 = false, d155 = false, d165 = false, d215 = false, d225 = false, d235 = false, d245 = false, d255 = false, d265 = false, d315 = false, d325 = false, d335 = false, d345 = false, d355 = false, d365 = false, d415 = false, d425 = false, d435 = false, d445 = false, d455 = false, d465 = false;
bool d116 = false, d126 = false, d136 = false, d146 = false, d156 = false, d166 = false, d216 = false, d226 = false, d236 = false, d246 = false, d256 = false, d266 = false, d316 = false, d326 = false, d336 = false, d346 = false, d356 = false, d366 = false, d416 = false, d426 = false, d436 = false, d446 = false, d456 = false, d466 = false;
bool d117 = false, d127 = false, d137 = false, d147 = false, d157 = false, d167 = false, d217 = false, d227 = false, d237 = false, d247 = false, d257 = false, d267 = false, d317 = false, d327 = false, d337 = false, d347 = false, d357 = false, d367 = false, d417 = false, d427 = false, d437 = false, d447 = false, d457 = false, d467 = false;
bool d118 = false, d128 = false, d138 = false, d148 = false, d158 = false, d168 = false, d218 = false, d228 = false, d238 = false, d248 = false, d258 = false, d268 = false, d318 = false, d328 = false, d338 = false, d348 = false, d358 = false, d368 = false, d418 = false, d428 = false, d438 = false, d448 = false, d458 = false, d468 = false;
bool d119 = false, d129 = false, d139 = false, d149 = false, d159 = false, d169 = false, d219 = false, d229 = false, d239 = false, d249 = false, d259 = false, d269 = false, d319 = false, d329 = false, d339 = false, d349 = false, d359 = false, d369 = false, d419 = false, d429 = false, d439 = false, d449 = false, d459 = false, d469 = false;
bool dc11 = false, dc12 = false, dc13 = false, dc14 = false, dc15 = false, dc16 = false, dc21 = false, dc22 = false, dc23 = false, dc24 = false, dc25 = false, dc26 = false, dc31 = false, dc32 = false, dc33 = false, dc34 = false, dc35 = false, dc36 = false, dc41 = false, dc42 = false, dc43 = false, dc44 = false, dc45 = false, dc46 = false;


while(start)
{
    while(menu)
    {
        ALLEGRO_EVENT ev;

        al_wait_for_event(event_queue, &ev);
        al_show_mouse_cursor(display);

        if(ev.type == ALLEGRO_EVENT_TIMER)
        {
            al_play_sample_instance(inst_somtrilha);

            al_clear_to_color(al_map_rgb(0,100,0));
            al_draw_bitmap(imgmenu, 0, 0, 0);
            al_flip_display();
        }
        else if(ev.type==ALLEGRO_EVENT_KEY_DOWN)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_ESCAPE:
                fim = true;
                menu = false;
                break;
            }
        }
        else if(ev.type==ALLEGRO_EVENT_MOUSE_BUTTON_DOWN)
        {
            switch(ev.mouse.button)
            {
            case 1:
                if(ev.mouse.x >= 58 && ev.mouse.y >= 549 && ev.mouse.x <= 315 && ev.mouse.y <= 609) //JOGAR
                {
                    introducao = true;
                    menu = false;
                    break;
                }
                else if(ev.mouse.x >= 383 && ev.mouse.y >= 549 && ev.mouse.x <= 639 && ev.mouse.y <= 609) //INSTRUCOES
                {
                    instrucoes = true;
                    menu = false;
                    break;
                }
                else if(ev.mouse.x >= 714 && ev.mouse.y >= 549 && ev.mouse.x <= 970 && ev.mouse.y <= 609) //SAIR
                {
                    fim = true;
                    menu = false;
                    break;
                }
            }
        }
    }

    while(instrucoes)
    {
        ALLEGRO_EVENT ev;

        al_wait_for_event(event_queue, &ev);
        al_hide_mouse_cursor(display);

        if(ev.type == ALLEGRO_EVENT_TIMER)
        {
            al_clear_to_color(al_map_rgb(0,100,0));
            al_draw_bitmap(imginstrucoes, 0, 0, 0);
            al_flip_display();
        }
        else if(ev.type == ALLEGRO_EVENT_KEY_DOWN)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_ENTER:
                introducao = true;
                instrucoes = false;
                break;

            case ALLEGRO_KEY_ESCAPE:
                fim = true;
                instrucoes = false;
                break;
            }
        }
    }

    while(introducao)
    {
        ALLEGRO_EVENT ev;

        al_wait_for_event(event_queue, &ev);
        al_hide_mouse_cursor(display);

        if(ev.type == ALLEGRO_EVENT_TIMER)
        {
        al_play_sample_instance(inst_somtrilha);

        al_clear_to_color(al_map_rgb(0,100,0));

        switch(level)
        {
        case 1:
            al_draw_bitmap(missao1, 0, 0, 0);
            break;

        case 2:
            al_draw_bitmap(missao2, 0, 0, 0);
            break;

        case 3:
            al_draw_bitmap(missao3, 0, 0, 0);
            break;

        case 4:
            al_draw_bitmap(missao4, 0, 0, 0);
            break;
        }

        astronauta_pos_x = SCREEN_W - HATCH_LARGURA / 2.0 - ASTRONAUTA_LARGURA / 2.0;
        astronauta_pos_y = SCREEN_H;

        al_flip_display();
        }
        else if(ev.type == ALLEGRO_EVENT_KEY_DOWN)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_ENTER:
                saida = true;
                introducao = false;
                break;

            case ALLEGRO_KEY_ESCAPE:
                fim = true;
                introducao = false;
                break;
            }
        }
    }

    while(saida)
    {
        ALLEGRO_EVENT ev;

        al_stop_sample_instance(inst_somtrilha);

        al_wait_for_event(event_queue, &ev);
        al_hide_mouse_cursor(display);

        if(ev.type == ALLEGRO_EVENT_TIMER)
        {
            al_clear_to_color(al_map_rgb(0,100,0));

            for( ; astronauta_pos_y > 628; astronauta_pos_y -= 2)
            {
                al_draw_bitmap(background, 0, 0, 0);

                switch(level)
                {
                case 1:
                    al_draw_bitmap(satelite1, satelite1_pos_x, satelite1_pos_y, 0);
                    break;

                case 2:
                    al_draw_bitmap(satelite2, satelite2_pos_x, satelite2_pos_y, 0);
                    break;

                case 3:
                    al_draw_bitmap(satelite3, satelite3_pos_x, satelite3_pos_y, 0);
                    break;

                case 4:
                    al_draw_bitmap(satelite4, satelite4_pos_x, satelite4_pos_y, 0);
                    break;
                }
                al_draw_bitmap(astronautaesq, astronauta_pos_x, astronauta_pos_y, 0);
                al_draw_bitmap(hatch, hatch_pos_x, hatch_pos_y, 0);
                sleep(0.75);
                al_flip_display();
            }
            jogo = true;
            missao = true;
            movimento = true;
            saida = false;
            lado = false;
        }
        else if(ev.type == ALLEGRO_EVENT_KEY_DOWN && movimento)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_UP:
                break;
            case ALLEGRO_KEY_DOWN:
                break;
            case ALLEGRO_KEY_LEFT:
                break;
            case ALLEGRO_KEY_RIGHT:
                break;
            case ALLEGRO_KEY_ENTER:
                break;
            case ALLEGRO_KEY_ESCAPE:
                saida = false;
                fim = true;
                break;
            }
        }
        else if(ev.type == ALLEGRO_EVENT_KEY_UP && movimento)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_UP:
                break;
            case ALLEGRO_KEY_DOWN:
                break;
            case ALLEGRO_KEY_LEFT:
                break;
            case ALLEGRO_KEY_RIGHT:
                break;
            }
        }
    }

    while(entrada)
    {
        ALLEGRO_EVENT ev;

        al_wait_for_event(event_queue, &ev);
        al_hide_mouse_cursor(display);

        if(ev.type == ALLEGRO_EVENT_TIMER)
        {
            for( ;astronauta_pos_x < SCREEN_W - HATCH_LARGURA / 2.0 - ASTRONAUTA_LARGURA / 2.0; astronauta_pos_x += 2)
            {
                al_draw_bitmap(background, 0, 0, 0);
                switch(level)
                {
                case 1:
                    al_draw_bitmap(satelite1, satelite1_pos_x, satelite1_pos_y, 0);
                    break;

                case 2:
                    al_draw_bitmap(satelite2, satelite2_pos_x, satelite2_pos_y, 0);
                    break;

                case 3:
                    al_draw_bitmap(satelite3, satelite3_pos_x, satelite3_pos_y, 0);
                    break;

                case 4:
                    al_draw_bitmap(satelite4, satelite4_pos_x, satelite4_pos_y, 0);
                    break;
                }
                al_draw_bitmap(hatch, hatch_pos_x, hatch_pos_y, 0);
                al_draw_bitmap(astronautadir, astronauta_pos_x, astronauta_pos_y, 0);
                sleep(0.75);
                al_flip_display();
            }
            for( ;astronauta_pos_x > SCREEN_W - HATCH_LARGURA / 2.0 - ASTRONAUTA_LARGURA / 2.0; astronauta_pos_x -= 2)
            {
                al_draw_bitmap(background, 0, 0, 0);
                switch(level)
                {
                case 1:
                    al_draw_bitmap(satelite1, satelite1_pos_x, satelite1_pos_y, 0);
                    break;

                case 2:
                    al_draw_bitmap(satelite2, satelite2_pos_x, satelite2_pos_y, 0);
                    break;

                case 3:
                    al_draw_bitmap(satelite3, satelite3_pos_x, satelite3_pos_y, 0);
                    break;

                case 4:
                    al_draw_bitmap(satelite4, satelite4_pos_x, satelite4_pos_y, 0);
                    break;
                }
                al_draw_bitmap(hatch, hatch_pos_x, hatch_pos_y, 0);
                al_draw_bitmap(astronautaesq, astronauta_pos_x, astronauta_pos_y, 0);
                sleep(0.75);
                al_flip_display();
            }
            for( ;astronauta_pos_y < SCREEN_H - HATCH_ALTURA - ASTRONAUTA_ALTURA; astronauta_pos_y += 2)
            {
                al_draw_bitmap(background, 0, 0, 0);
                switch(level)
                {
                case 1:
                    al_draw_bitmap(satelite1, satelite1_pos_x, satelite1_pos_y, 0);
                    break;

                case 2:
                    al_draw_bitmap(satelite2, satelite2_pos_x, satelite2_pos_y, 0);
                    break;

                case 3:
                    al_draw_bitmap(satelite3, satelite3_pos_x, satelite3_pos_y, 0);
                    break;

                case 4:
                    al_draw_bitmap(satelite4, satelite4_pos_x, satelite4_pos_y, 0);
                    break;
                }
                al_draw_bitmap(hatch, hatch_pos_x, hatch_pos_y, 0);
                al_draw_bitmap(astronautadir, astronauta_pos_x, astronauta_pos_y, 0);
                sleep(0.75);
                al_flip_display();
            }
            for( ;astronauta_pos_y > SCREEN_H - HATCH_ALTURA - ASTRONAUTA_ALTURA; astronauta_pos_y -= 2)
            {
                al_draw_bitmap(background, 0, 0, 0);
                switch(level)
                {
                case 1:
                    al_draw_bitmap(satelite1, satelite1_pos_x, satelite1_pos_y, 0);
                    break;

                case 2:
                    al_draw_bitmap(satelite2, satelite2_pos_x, satelite2_pos_y, 0);
                    break;

                case 3:
                    al_draw_bitmap(satelite3, satelite3_pos_x, satelite3_pos_y, 0);
                    break;

                case 4:
                    al_draw_bitmap(satelite4, satelite4_pos_x, satelite4_pos_y, 0);
                    break;
                }
                al_draw_bitmap(hatch, hatch_pos_x, hatch_pos_y, 0);
                al_draw_bitmap(astronautadir, astronauta_pos_x, astronauta_pos_y, 0);
                sleep(0.75);
                al_flip_display();
            }

            for( ; astronauta_pos_y < SCREEN_H; astronauta_pos_y += 2)
            {
                al_draw_bitmap(background, 0, 0, 0);
                switch(level)
                {
                case 1:
                    al_draw_bitmap(satelite1, satelite1_pos_x, satelite1_pos_y, 0);
                    break;

                case 2:
                    al_draw_bitmap(satelite2, satelite2_pos_x, satelite2_pos_y, 0);
                    break;

                case 3:
                    al_draw_bitmap(satelite3, satelite3_pos_x, satelite3_pos_y, 0);
                    break;

                case 4:
                    al_draw_bitmap(satelite4, satelite4_pos_x, satelite4_pos_y, 0);
                    break;
                }
                al_draw_bitmap(astronautadir, astronauta_pos_x, astronauta_pos_y, 0);
                al_draw_bitmap(hatch, hatch_pos_x, hatch_pos_y, 0);
                sleep(0.75);
                al_flip_display();
            }

            if(astronauta_pos_x == SCREEN_W - HATCH_LARGURA / 2.0 - ASTRONAUTA_LARGURA / 2.0 && astronauta_pos_y == SCREEN_H)
            {
                fim = true;
                movimento = false;
                entrada = false;

                if(penalidade == 0)
                {
                    para_bens = true;
                }
                else
                {
                    e_tabom = true;
                }
                break;
            }
        }
        else if(ev.type == ALLEGRO_EVENT_KEY_DOWN && movimento)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_UP:
                break;
            case ALLEGRO_KEY_DOWN:
                break;
            case ALLEGRO_KEY_LEFT:
                break;
            case ALLEGRO_KEY_RIGHT:
                break;
            case ALLEGRO_KEY_ENTER:
                break;
            case ALLEGRO_KEY_ESCAPE:
                entrada = false;
                fim = true;
                break;
            }
        }
        else if(ev.type == ALLEGRO_EVENT_KEY_UP && movimento)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_UP:
                break;
            case ALLEGRO_KEY_DOWN:
                break;
            case ALLEGRO_KEY_LEFT:
                break;
            case ALLEGRO_KEY_RIGHT:
                break;
            }
        }
    }

    while(jogo)
    {
        ALLEGRO_EVENT ev;

        al_wait_for_event(event_queue, &ev);
        al_hide_mouse_cursor(display);

        if(ev.type == ALLEGRO_EVENT_TIMER)
        {
            if(missao)
            {
                switch(level)
                {
                case 1:
                    if(astronauta_pos_x + ASTRONAUTA_LARGURA/2.0 >= 150 && astronauta_pos_x + ASTRONAUTA_LARGURA/2.0 <= 402 && astronauta_pos_y + ASTRONAUTA_ALTURA/2.0 >= 80 && astronauta_pos_y + ASTRONAUTA_ALTURA/2.0 <= 294)
                    {
                        astronauta_vy = 0;
                        astronauta_vx = 0;
                    if(!drawreparo)
                    {
                        al_draw_bitmap(laptop, laptop_pos_x, laptop_pos_y, 0);
                        drawreparo = true;
                    }
                        reparo = true;
                        movimento = false;
                        jogo = false;
                        break;
                    }
                    break;

                case 2:
                    if(astronauta_pos_x + ASTRONAUTA_LARGURA/2.0 >= 100 && astronauta_pos_x + ASTRONAUTA_LARGURA/2.0 <= 352 && astronauta_pos_y + ASTRONAUTA_ALTURA/2.0 >= 150 && astronauta_pos_y + ASTRONAUTA_ALTURA/2.0 <= 366)
                    {
                        astronauta_vy = 0;
                        astronauta_vx = 0;
                        reparo = true;
                        movimento = false;
                        jogo = false;
                        break;
                    }
                    break;

                case 3:
                    if(astronauta_pos_x + ASTRONAUTA_LARGURA/2.0 >= 40 && astronauta_pos_x + ASTRONAUTA_LARGURA/2.0 <= 292 && astronauta_pos_y + ASTRONAUTA_ALTURA/2.0 >= 400 && astronauta_pos_y + ASTRONAUTA_ALTURA/2.0 <= 696)
                    {
                        astronauta_vy = 0;
                        astronauta_vx = 0;
                        reparo = true;
                        movimento = false;
                        jogo = false;
                        break;
                    }
                    break;

                case 4:
                    if(astronauta_pos_x + ASTRONAUTA_LARGURA/2.0 >= 10 && astronauta_pos_x + ASTRONAUTA_LARGURA/2.0 <= 262 && astronauta_pos_y + ASTRONAUTA_ALTURA/2.0 >= 10 && astronauta_pos_y + ASTRONAUTA_ALTURA/2.0 <= 416)
                    {
                        astronauta_vy = 0;
                        astronauta_vx = 0;
                        reparo = true;
                        movimento = false;
                        jogo = false;
                        break;
                    }
                    break;
                }
            }

            if(volta)
            {
                if(astronauta_pos_x + ASTRONAUTA_LARGURA/2.0 >= 825 && astronauta_pos_x + ASTRONAUTA_LARGURA/2.0 <= 1218 && astronauta_pos_y + ASTRONAUTA_ALTURA/2.0 >= 660 && astronauta_pos_y + ASTRONAUTA_ALTURA/2.0 <= 914)
                {
                    astronauta_vy = 0;
                    astronauta_vx = 0;
                    volta = false;
                    jogo = false;
                    entrada = true;

                    resultado = 1;

                    if(fim4) //GG
                    {
                        resultado = 3;
                    }

                    break;
                }
            }

            astronauta_pos_y  += astronauta_vy;
            astronauta_pos_x  += astronauta_vx;
            redraw = true;
        }
        else if(ev.type == ALLEGRO_EVENT_KEY_DOWN && movimento)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_UP:
                astronauta_vy = -2;
                break;

            case ALLEGRO_KEY_DOWN:
                astronauta_vy = 2;
                break;

            case ALLEGRO_KEY_LEFT:
                astronauta_vx = -2;
                lado = false;
                break;

            case ALLEGRO_KEY_RIGHT:
                astronauta_vx = 2;
                lado = true;
                break;

            case ALLEGRO_KEY_ENTER:
                break;

            case ALLEGRO_KEY_ESCAPE:
                jogo = false;
                fim = true;
                break;
            }
        }
        else if(ev.type == ALLEGRO_EVENT_KEY_UP && movimento)
        {
            switch(ev.keyboard.keycode)
            {
            case ALLEGRO_KEY_UP:
                astronauta_vy = 0;
                break;

            case ALLEGRO_KEY_DOWN:
                astronauta_vy = 0;
                break;

            case ALLEGRO_KEY_LEFT:
                astronauta_vx = 0;
                break;

            case ALLEGRO_KEY_RIGHT:
                astronauta_vx = 0;
                break;
            }
        }
        else if(ev.type == ALLEGRO_EVENT_DISPLAY_CLOSE)
        {
            break;
        }

        if(redraw && al_is_event_queue_empty(event_queue))
        {
            redraw = false;

            al_clear_to_color(al_map_rgb(0,100,0));
            al_draw_bitmap(background, 0, 0, 0);
            al_draw_bitmap(hatch, hatch_pos_x, hatch_pos_y, 0);
            switch(level)
            {
            case 1:
                al_draw_bitmap(satelite1, satelite1_pos_x, satelite1_pos_y, 0);
                break;

            case 2:
                al_draw_bitmap(satelite2, satelite2_pos_x, satelite2_pos_y, 0);
                break;

            case 3:
                al_draw_bitmap(satelite3, satelite3_pos_x, satelite3_pos_y, 0);
                break;

            case 4:
                al_draw_bitmap(satelite4, satelite4_pos_x, satelite4_pos_y, 0);
                break;
            }

            if(lado)
            {
                al_draw_bitmap(astronautadir, astronauta_pos_x, astronauta_pos_y, 0);
            }
            else
            {
                al_draw_bitmap(astronautaesq, astronauta_pos_x, astronauta_pos_y, 0);
            }

            al_flip_display();
        }
    }

    while(reparo)
    {
        ALLEGRO_EVENT ev;

        al_wait_for_event(event_queue, &ev);
        al_hide_mouse_cursor(display);

        if(ev.type == ALLEGRO_EVENT_TIMER)
        {
            if(!posicoes)
            {
                switch(level)
                {
                case 1:
                    d11 = true;
                    d12 = true;
                    d13 = true;
                    d14 = true;
                    d15 = true;
                    d16 = true;
                    fim1 = true;
                    break;

                case 2:
                    d11 = true;
